{
  "name": "grpc-man",
  "version": "1.4.2",
  "description": "A client to call gRPC server methods on the fly. 一个 gRPC 客户端，可以灵活地调用 gRPC 服务。",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": "lib/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint --fix -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jeff-Tian/grpc-man.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Jeff-Tian/grpc-man/issues"
  },
  "homepage": "https://github.com/Jeff-Tian/grpc-man#readme",
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.1",
    "@types/protobufjs": "^6.0.0",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-sonarts": "^1.8.0",
    "typescript": "^3.1.5",
    "semantic-release": "^15.13.24"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.3.0",
    "grpc": "~1.23.0"
  }
}
